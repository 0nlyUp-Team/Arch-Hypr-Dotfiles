{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1c141490",
                "editor.background": "#1c141490",
                "editor.foreground": "#c6c4c4",
                "text": "#c6c4c4",
                "text.muted": "#c6c4c470",
                "text.ignored": "#c6c4c440",
                "text.placeholder": "#c6c4c450",
                "ignored": "#c6c4c430",
                "element.hover": "#c6c4c430",
                "ghost_element.hover": "#E7467530",
                "ghost_element.selected": "#E7467530",
                "ghost_element.active": "#E7467560",
                "border": "#1c1414",
                "editor.highlighted_line.background": "#E7467510",
                "editor.active_line.background": "#E7467510",
                "panel.background": "#1c141490",
                "title_bar.background": "#1c141490",
                "title_bar.inactive_background": "#1c141490",
                "status_bar.background": "#1c141490",
                "drop_target.background": "#1c141490",
                "elevated_surface.background": "#1c1414",
                "toolbar.background": "#1c141490",
                "tab_bar.background": "#1c141490",
                "tab.inactive_background": "#1c141490",
                "tab.active_background": "#E7467530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1c1414",
                "scrollbar.thumb.background": "#E74675",
                "editor.gutter.background": "#1c141490",
                "terminal.background": "#1c141410",
                "terminal.foreground": "#c6c4c4",
                "terminal.dim_foreground": "#c6c4c4",
                "terminal.bright_foreground": "#c6c4c4",
                "terminal.ansi.black": "#1c1414",
                "terminal.ansi.red": "#E74675",
                "terminal.ansi.green": "#B06B8A",
                "terminal.ansi.yellow": "#EE5D8E",
                "terminal.ansi.blue": "#988382",
                "terminal.ansi.magenta": "#A89698",
                "terminal.ansi.cyan": "#D39BA9",
                "terminal.ansi.white": "#c6c4c4",
                "terminal.ansi.bright_black": "#725e5e",
                "terminal.ansi.bright_red": "#E74675",
                "terminal.ansi.bright_green": "#B06B8A",
                "terminal.ansi.bright_yellow": "#EE5D8E",
                "terminal.ansi.bright_blue": "#988382",
                "terminal.ansi.bright_magenta": "#A89698",
                "terminal.ansi.bright_cyan": "#D39BA9",
                "terminal.ansi.bright_white": "#c6c4c4",
                "modified": "#E74675",
                "syntax": {
                    "attribute": {
                        "color": "#E74675"
                    },
                    "boolean": {
                      "color": "#B06B8A"
                    },
                    "comment": {
                      "color": "#725e5e"
                    },
                    "comment.doc": {
                      "color": "#725e5e"
                    },
                    "constant": {
                      "color": "#B06B8A"
                    },
                    "function": {
                      "color": "#D39BA9"
                    },
                    "keyword": {
                      "color": "#988382"
                    },
                    "number": {
                      "color": "#A89698"
                    },
                    "operator": {
                      "color": "#988382"
                    },
                    "string": {
                      "color": "#E74675"
                    },
                    "variable": {
                      "color": "#B06B8A"
                    }
                },
                "players": [
                  {
                    "cursor": "#E74675",
                    "background": "#1c1414",
                    "selection": "#E7467530"
                  }
                ]
            }
        }
    ]
}
