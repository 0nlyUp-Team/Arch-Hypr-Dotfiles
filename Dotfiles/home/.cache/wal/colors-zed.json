{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a0a1190",
                "editor.background": "#0a0a1190",
                "editor.foreground": "#c1c1c3",
                "text": "#c1c1c3",
                "text.muted": "#c1c1c370",
                "text.ignored": "#c1c1c340",
                "text.placeholder": "#c1c1c350",
                "ignored": "#c1c1c330",
                "element.hover": "#c1c1c330",
                "ghost_element.hover": "#52556930",
                "ghost_element.selected": "#52556930",
                "ghost_element.active": "#52556960",
                "border": "#0a0a11",
                "editor.highlighted_line.background": "#52556910",
                "editor.active_line.background": "#52556910",
                "panel.background": "#0a0a1190",
                "title_bar.background": "#0a0a1190",
                "title_bar.inactive_background": "#0a0a1190",
                "status_bar.background": "#0a0a1190",
                "drop_target.background": "#0a0a1190",
                "elevated_surface.background": "#0a0a11",
                "toolbar.background": "#0a0a1190",
                "tab_bar.background": "#0a0a1190",
                "tab.inactive_background": "#0a0a1190",
                "tab.active_background": "#52556930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a0a11",
                "scrollbar.thumb.background": "#525569",
                "editor.gutter.background": "#0a0a1190",
                "terminal.background": "#0a0a1110",
                "terminal.foreground": "#c1c1c3",
                "terminal.dim_foreground": "#c1c1c3",
                "terminal.bright_foreground": "#c1c1c3",
                "terminal.ansi.black": "#0a0a11",
                "terminal.ansi.red": "#525569",
                "terminal.ansi.green": "#61647A",
                "terminal.ansi.yellow": "#6F738A",
                "terminal.ansi.blue": "#7B7DA1",
                "terminal.ansi.magenta": "#7E819B",
                "terminal.ansi.cyan": "#8F8EAC",
                "terminal.ansi.white": "#c1c1c3",
                "terminal.ansi.bright_black": "#57576b",
                "terminal.ansi.bright_red": "#525569",
                "terminal.ansi.bright_green": "#61647A",
                "terminal.ansi.bright_yellow": "#6F738A",
                "terminal.ansi.bright_blue": "#7B7DA1",
                "terminal.ansi.bright_magenta": "#7E819B",
                "terminal.ansi.bright_cyan": "#8F8EAC",
                "terminal.ansi.bright_white": "#c1c1c3",
                "modified": "#525569",
                "syntax": {
                    "attribute": {
                        "color": "#525569"
                    },
                    "boolean": {
                      "color": "#61647A"
                    },
                    "comment": {
                      "color": "#57576b"
                    },
                    "comment.doc": {
                      "color": "#57576b"
                    },
                    "constant": {
                      "color": "#61647A"
                    },
                    "function": {
                      "color": "#8F8EAC"
                    },
                    "keyword": {
                      "color": "#7B7DA1"
                    },
                    "number": {
                      "color": "#7E819B"
                    },
                    "operator": {
                      "color": "#7B7DA1"
                    },
                    "string": {
                      "color": "#525569"
                    },
                    "variable": {
                      "color": "#61647A"
                    }
                },
                "players": [
                  {
                    "cursor": "#525569",
                    "background": "#0a0a11",
                    "selection": "#52556930"
                  }
                ]
            }
        }
    ]
}
